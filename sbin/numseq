#!run
let c=0;
export async function main(){
    for(let s of gen(3)){
        this.echo(...s);
        await this.sleep(1);
    }
}
function *gen(dim, sum){
    //c++;
    //if(c>10)throw c ;
    if(sum==null){
        let sum=0;
        while(true){
            yield*gen(dim,sum);
            sum++;
        }
    }
    if(dim==1){
        yield [sum];
        return ;
    }
    for(let i=0;i<=sum;i++){
        for(let s of gen(dim-1,i)){
            yield [...s,sum-i];
        }
    }
}
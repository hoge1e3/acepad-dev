        this.edit(f);
        return;
    }
    const mv=this.resolve("../bin/mv").text();
    this.echo(mv.includes("\r"), mv.includes("\n"));
    return;
    
    if(options?.err)options.a.b;
    const niseError={message:"The error", stack:""};
    for (let m of loadedModules()) {
        if (!m.entry) continue;
        niseError.stack+="@"+m.url+"\n";
        if (niseError.stack.length>200) break;
    }
    console.log(niseError.stack);
    convertStack(niseError, true);
    console.log(niseError.stack);
    return;
    console.log(...loadedModules());
    const cmd="python \r/jsmod/node_modules/@acepad/samples/samples/test.py";
    cmd.replace(/^\s*(\S+)(\s.*)?$/,(_,c,a)=>{
        alert(c);
            //console.log("{"+c+"}");
            if(sh.hasCmd(c)){
                res=c+(a||"");
            }
        });
        
    console.log(loadedModules());
    console.log(assert);
    console.log(assert.ok);
    assert.ok("test");
    assert.ok(false, "fail");
    window.p1=p("321",{
        "ho": p("0.2023467592349557",{
            
        }),
    });
    
    return ;
}

#!run
import {unlinkSync} from "fs";
export async function main(...args) {
    args=this.collectOptions(args);
    const options=args.pop();
    let files=args;
    let sum=0;
    for(let file of files){
        file=this.resolve(file, true);
        if(file.isLink()){
            unlinkSync(file.path());
        }else{
        sum+=file.rm(options);
        }
    }
    return sum;

}
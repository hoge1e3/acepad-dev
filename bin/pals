//!run

export async function main(){
let p=[[0,0,0],
[64,183,74],[117,207,126],[89,86,215],
[128,119,239],[184,95,81],
[102,219,238],[217,102,90],[253,138,126],
[204,195,96],[222,208,136],
[59,161,66],[182,103,180],
[204,204,204],[255,255,255]];
for(let i=0;i<15;i++){
    
    this.echo(i+1,"#"+p[i].map(pad).join(""));
}
}
function pad(n){
    n="0"+n.toString(16);
    n=n.substring(n.length-2);
    return n;
}